{
    "name": "horde/cli_modular",
    "description": "Modular command line interface library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Cli_Modular",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Gunnar Wrobel",
            "email": "p@rdus.de",
            "role": "lead"
        }
    ],
    "time": "2022-08-25",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/argv": "^3 || dev-FRAMEWORK_6_0"
    },
    "require-dev": {
        "horde/test": "dev-FRAMEWORK_6_0"
    },
    "suggest": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "autoload": {
        "psr-0": {
            "Horde_Cli_Modular": "lib/"
        },
        "psr-4": {
            "Horde\\Cli\\Modular\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Cli\\Modular\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    },
    "minimum-stability": "dev"
}
